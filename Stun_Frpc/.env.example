# ====================================
# 客户端 (Stun_Frpc) 配置
#（修改后将.env.example重命名为.env）
# ====================================

# 客户端编号 (必填，多客户端时设置不同编号: 1, 2, 3...)
STUN_CLIENT_NUMBER=1

# 服务端域名 (必填，与服务端保持一致)
STUN_DOMAIN=

# 定期检查间隔(秒)
STUN_CHECK_INTERVAL=120

# FRP 认证 Token (必须与服务端一致)
FRP_AUTH_TOKEN=
